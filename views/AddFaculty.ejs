<%- include('partials/headerAdmin.ejs') %>
<div align="center">
	<form class="contact1-form validate-form" action="addFaculty" method="post">
					<span class="contact1-form-title">
						Add Faculty
					</span>
	
					<div class="wrap-input1 validate-input" data-validate = "Name is required">
						<input class="input1" type="text" name="facName" placeholder="Faculty Name">
						<span class="shadow-input1"></span>
					</div>
					
					<div class="wrap-input1 validate-input" data-validate = "Email is required">
						<input class="input1" type="text" name="facEmail" placeholder="Faculty E-Mail">
						<span class="shadow-input1"></span>
					</div>
					
					<div class="wrap-input1 validate-input" data-validate = "phone numbet is required">
						<input class="input1" type="text" name="userName" placeholder="Phone number">
						<span class="shadow-input1"></span>
					</div>
		
	
					<div class="wrap-input1 validate-input" data-validate = "Password is required">
						<input class="input1" type="password" name="password" placeholder="Password">
						<span class="shadow-input1"></span>
					</div>
					
					<div class="wrap-input1 validate-input" data-validate = "Picture is required">
						<input class="input1" type="text" name="userName" placeholder="Picture">
						<span class="shadow-input1"></span>
					</div>
	
					
					<div class="wrap-input1 validate-input" data-validate = "required">
						<!--<textarea class="input1" name="message" placeholder="Message"></textarea>-->
						<select class="input1">
							<option>--Select Faculty Type--</option>
							<option>okk</option>
							<option>okk2</option>
							<option>okk3</option>
						</select>
						<span class="shadow-input1"></span>
					</div>
					<div class="container-contact1-form-btn">
						<button class="contact1-form-btn" onclick="addFaculty">
							<span>
								ADD
								<i class="fa fa-long-arrow-right" aria-hidden="true"></i>
							</span>
						</button>
					</div>
					
				</form>
				</div>
	
	
	
	
	</div></div></div>